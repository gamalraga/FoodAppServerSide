    <fragment
        android:id="@+id/nav_myAccount"
        android:name="gamal.myappnew.serverforclientapp.ui.MyAcount.MyAccountFragment"
        android:label="@string/menu_share"
        tools:layout="@layout/fragment_myaccount" />

    <fragment
        android:id="@+id/nav_requests"
        android:name="gamal.myappnew.serverforclientapp.ui.Requests.RequestsFragment"
        android:label="@string/menu_send"
        tools:layout="@layout/fragment_requests" />

    <fragment
        android:id="@+id/nav_showfood"
        android:name="gamal.myappnew.serverforclientapp.Fragment.show_foodFragment"
        android:label="Wishlist"
        tools:layout="@layout/fragment_show_food" />
    <fragment
        android:id="@+id/update_category"
        android:name="gamal.myappnew.serverforclientapp.Fragment.update_category"
        android:label="Update category"
        tools:layout="@layout/update_category" />

