<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="gamal.myappnew.serverforclientapp.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_category"
        android:name="gamal.myappnew.serverforclientapp.ui.Category.CategoryFragment"
        android:label="@string/menu_gallery"
        tools:layout="@layout/fragment_category" />

    <fragment
        android:id="@+id/nav_foodlist"
        android:name="gamal.myappnew.serverforclientapp.ui.ListFood.ListFoodFragment"
        android:label="@string/menu_slideshow"
        tools:layout="@layout/fragment_listfood" />
    <fragment
        android:id="@+id/nav_creatShipper"
        android:name="gamal.myappnew.serverforclientapp.ui.CreateShiper.CreateShiperFragment"
